 00B9 08            push  A
 00BA 28            romx                               ; get the MSB of xidata's address
 00BB 5300          mov   [__r0], A
 00BD 18            pop   A
 00BE 75            inc   X
 00BF 0900          adc   A, 0
 00C1 28            romx                               ; get the LSB of xidata's address
 00C2 4B            swap  A, X
 00C3 5100          mov   A, [__r0]                    ; pXIData (in [A,X]) points to the
 00C5                                                  ;   XIData structure list in flash
 00C5 8004          jmp   .AccessStruct
 00C7           
 00C7               ; Unpack one element in the xidata "structure list" that specifies the
 00C7               ; values of C variables. Each structure contains 3 member elements.
 00C7               ; The first is a pointer to a contiguous block of RAM to be initial-
 00C7               ; ized. Blocks are always 255 bytes or less in length and never cross
 00C7               ; RAM page boundaries. The list terminates when the MSB of the pointer
 00C7               ; contains 0xFF. There are two formats for the struct depending on the
 00C7               ; value in the second member element, an unsigned byte:
 00C7               ; (1) If the value of the second element is non-zero, it represents
 00C7               ; the 'size' of the block of RAM to be initialized. In this case, the
 00C7               ; third member of the struct is an array of bytes of length 'size' and
 00C7               ; the bytes are copied to the block of RAM.
 00C7               ; (2) If the value of the second element is zero, the block of RAM is
 00C7               ; to be cleared to zero. In this case, the third member of the struct
 00C7               ; is an unsigned byte containing the number of bytes to clear.
 00C7           
 00C7           .AccessNextStructLoop:
 00C7 75            inc   X                            ; pXIData++
 00C8 0900          adc   A, 0
 00CA           .AccessStruct:                         ; Entry point for first block
 00CA               ;
 00CA               ; Assert: pXIData in [A,X] points to the beginning of an XIData struct.
 00CA               ;
 00CA 62E300        mov   reg[RES_WDT], 00h
 00CD 08            push  A
 00CE 28            romx                               ; MSB of RAM addr (CPU.A <- *pXIData)
 00CF 60D5          mov   reg[MVW_PP], A               ;   for use with MVI write operations
 00D1 74            inc   A                            ; End of Struct List? (MSB==0xFF?)
 00D2 A04B          jz    .C_RTE_WrapUp                ;   Yes, C runtime environment complete
 00D4 18            pop   A                            ; restore pXIData to [A,X]
 00D5 75            inc   X                            ; pXIData++
 00D6 0900          adc   A, 0
 00D8 08            push  A
 00D9 28            romx                               ; LSB of RAM addr (CPU.A <- *pXIData)
 00DA 5300          mov   [__r0], A                    ; RAM Addr now in [reg[MVW_PP],[__r0]]
 00DC 18            pop   A                            ; restore pXIData to [A,X]
 00DD 75            inc   X                            ; pXIData++ (point to size)
 00DE 0900          adc   A, 0
 00E0 08            push  A
 00E1 28            romx                               ; Get the size (CPU.A <- *pXIData)
 00E2 A01C          jz    .ClearRAMBlockToZero         ; If Size==0, then go clear RAM
 00E4 5300          mov   [__r1], A                    ;             else downcount in __r1
 00E6 18            pop   A                            ; restore pXIData to [A,X]
 00E7           
 00E7           .CopyNextByteLoop:
 00E7               ; For each byte in the structure's array member, copy from flash to RAM.
 00E7               ; Assert: pXIData in [A,X] points to previous byte of flash source;
 00E7               ;         [reg[MVW_PP],[__r0]] points to next RAM destination;
 00E7               ;         __r1 holds a non-zero count of the number of bytes remaining.
 00E7               ;
 00E7 75            inc   X                            ; pXIData++ (point to next data byte)
 00E8 0900          adc   A, 0
 00EA 08            push  A
 00EB 28            romx                               ; Get the data value (CPU.A <- *pXIData)
 00EC 3F00          mvi   [__r0], A                    ; Transfer the data to RAM
 00EE 4700FF        tst   [__r0], 0xff                 ; Check for page crossing
 00F1 B006          jnz   .CopyLoopTail                ;   No crossing, keep going
 00F3 5DD5          mov   A, reg[ MVW_PP]              ;   If crossing, bump MVW page reg
 00F5 74            inc   A
 00F6 60D5          mov   reg[ MVW_PP], A
 00F8           .CopyLoopTail:
 00F8 18            pop   A                            ; restore pXIData to [A,X]
 00F9 7A00          dec   [__r1]                       ; End of this array in flash?
 00FB BFEB          jnz   .CopyNextByteLoop            ;   No,  more bytes to copy
 00FD 8FC9          jmp   .AccessNextStructLoop        ;   Yes, initialize another RAM block
 00FF           
 00FF           .ClearRAMBlockToZero:
 00FF 18            pop   A                            ; restore pXIData to [A,X]
 0100 75            inc   X                            ; pXIData++ (point to next data byte)
 0101 0900          adc   A, 0
 0103 08            push  A
 0104 28            romx                               ; Get the run length (CPU.A <- *pXIData)
 0105 5300          mov   [__r1], A                    ; Initialize downcounter
 0107 5000          mov   A, 0                         ; Initialize source data
 0109           
 0109           .ClearRAMBlockLoop:
 0109               ; Assert: [reg[MVW_PP],[__r0]] points to next RAM destination and
 0109               ;         __r1 holds a non-zero count of the number of bytes remaining.
 0109               ;
 0109 3F00          mvi   [__r0], A                    ; Clear a byte
 010B 4700FF        tst   [__r0], 0xff                 ; Check for page crossing
 010E B008          jnz   .ClearLoopTail               ;   No crossing, keep going
 0110 5DD5          mov   A, reg[ MVW_PP]              ;   If crossing, bump MVW page reg
 0112 74            inc   A
 0113 60D5          mov   reg[ MVW_PP], A
 0115 5000          mov   A, 0                         ; Restore the zero used for clearing
 0117           .ClearLoopTail:
 0117 7A00          dec   [__r1]                       ; Was this the last byte?
 0119 BFEF          jnz   .ClearRAMBlockLoop           ;   No,  continue
 011B 18            pop   A                            ;   Yes, restore pXIData to [A,X] and
 011C 8FAA          jmp   .AccessNextStructLoop        ;        initialize another RAM block
 011E           
 011E           .C_RTE_WrapUp:
 011E 18            pop   A                            ; balance stack
 011F           
                ENDIF ; SYSTEM_LARGE_MEMORY_MODEL
                
 011F           C_RTE_Done:
 011F           
                ENDIF ; C_LANGUAGE_SUPPORT
                
                
                    ;-------------------------------
                    ; Set Power-On Reset (POR) Level
                    ;-------------------------------
                    ;  The writes to the VLT_CR register below include setting the POR to VLT_CR_POR_HIGH,
                    ;  VLT_CR_POR_MID or VLT_CR_POR_LOW. Correctly setting this value is critical to the proper
                    ;  operation of the PSoC. The POR protects the M8C from mis-executing when Vdd falls low. 
                    ;  These values should not be changed from the settings here. See Section "POR and LVD" of 
                    ;  Technical Reference Manual #001-14463 for more information.
                
 011F 7110          or    F, FLAG_XIO_MASK
 0121           
                IF (POWER_SETTING & POWER_SET_3V3)             ; 3.3V Operation?
                    or   reg[VLT_CR], VLT_CR_POR_LOW           ;   Yes, change to midpoint trip
                ELSE                                                                               ; 5V Operation
                  IF ( CPU_CLOCK_JUST ^ OSC_CR0_CPU_24MHz )    ;      As fast as 24MHz?
 0121 43E300        or   reg[VLT_CR], VLT_CR_POR_LOW           ;         No, change to midpoint trip
                  ELSE ; 24HMz                                 ;
                    or    reg[VLT_CR], VLT_CR_POR_HIGH         ;        Yes, switch to  highest setting
                  ENDIF ; 24MHz
                ENDIF ; 3.3V Operation
                
 0124 70EF          and   F, ~FLAG_XIO_MASK
 0126           
 0126               ;----------------------------
 0126               ; Wrap up and invoke "main"
 0126               ;----------------------------
 0126           
 0126               ; Disable the Sleep interrupt that was used for timing above.  In fact,
 0126               ; no interrupts should be enabled now, so may as well clear the register.
 0126               ;
 0126 62E000        mov  reg[INT_MSK0],0
 0129           
 0129               ; Everything has started OK. Now select requested CPU & sleep frequency.
 0129               ;
 0129 7110          or    F, FLAG_XIO_MASK
 012B 62E000        mov  reg[OSC_CR0],(SLEEP_TIMER_JUST | CPU_CLOCK_JUST)
 012E 70EF          and   F, ~FLAG_XIO_MASK
 0130           
 0130               ; Global Interrupt are NOT enabled, this should be done in main().
 0130               ; LVD is set but will not occur unless Global Interrupts are enabled.
 0130               ; Global Interrupts should be enabled as soon as possible in main().
 0130               ;
 0130 62E200        mov  reg[INT_VC],0             ; Clear any pending interrupts which may
 0133                                              ; have been set during the boot process.
                IF      (TOOLCHAIN & HITECH)
                        ljmp  startup                  ; Jump to C compiler startup code
                ELSE
                IF ENABLE_LJMP_TO_MAIN
                    ljmp  _main                    ; goto main (no return)
                ELSE
 0133 7C0000        lcall _main                    ; call main
 0136           .Exit:
 0136 8FFF          jmp  .Exit                     ; Wait here after return till power-off or reset
                ENDIF
                ENDIF ; TOOLCHAIN
                
